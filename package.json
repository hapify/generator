{
  "name": "hapify-generator",
  "version": "0.4.0",
  "description": "Generator backend for Hapify server",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "watch": "rm -rf dist && tsc -w",
    "build": "rm -rf dist && ./node_modules/.bin/tsc -p .",
    "test": "./node_modules/.bin/lab tests/api",
    "lint": "./node_modules/.bin/prettier --write '**/*{.js,.ts}'"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/tractrs/hapify-generator.git"
  },
  "keywords": [
    "hapify",
    "generator",
    "backend"
  ],
  "author": "Edouard Demotes-Mainard <edouard@tractr.net>",
  "license": "ISC",
  "homepage": "https://bitbucket.org/tractrs/hapify-generator#readme",
  "devDependencies": {
    "@types/boom": "^7.2.1",
    "@types/hapi": "^17.8.0",
    "@types/joi": "^14.0.0",
    "code": "^5.2.4",
    "lab": "^18.0.0",
    "prettier": "^1.15.3",
    "reflect-metadata": "^0.1.12",
    "typedi": "^0.8.0",
    "typescript": "^3.2.2",
    "hapify-syntax": "file:./packages/hapify-syntax"
  },
  "dependencies": {
    "boom": "^7.3.0",
    "good": "^8.1.1",
    "good-console": "^7.1.0",
    "good-squeeze": "^5.1.0",
    "hapi": "^17.8.1",
    "joi": "^14.3.0",
    "safe-eval": "^0.4.1"
  },
  "engines": {
    "node": ">=9.2.0"
  }
